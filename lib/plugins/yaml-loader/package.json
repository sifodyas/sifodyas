{
  "name": "@sifodyas/yaml-loader",
  "version": "2.1.0",
  "description": "Add YamlLoader to Sifodyas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts"
  ],
  "scripts": {
    "_webpack": "TS_NODE_PROJECT=\"./webpack/tsconfig.json\" webpack --config ./webpack/webpack.config.ts",
    "clean": "rimraf dist/*",
    "prebuild": "yarn clean",
    "build": "yarn _webpack --env.production -p",
    "build:dev": "yarn prebuild && yarn _webpack --env.development"
  },
  "dependencies": {
    "@bios21/tstrait": "^1.1.0",
    "yamljs": "^0.3.0"
  },
  "peerDependencies": {
    "@sifodyas/sifodyas": "^2"
  }
}
